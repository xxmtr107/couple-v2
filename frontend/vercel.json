{
    "framework": "vite",
    "installCommand": "npm install --legacy-peer-deps && chmod -R +x node_modules/.bin || true",
    "buildCommand": "node node_modules/vite/bin/vite.js build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}